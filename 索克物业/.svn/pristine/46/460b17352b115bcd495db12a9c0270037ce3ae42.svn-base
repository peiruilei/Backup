/* pages/userCenter/customerServiceCenter/customerServiceCenter.wxss */

page {
  background-color: white;
}

.view-bg {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.btn-contact-customerService {
  width: 80%;
  margin-top: 200rpx;
  height: 160rpx;
  background-color: #68D6AB;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  border: none;
  border-radius: 16rpx;
}

.btn-contact-customerService::after
{
  border: none;
} 

.img-customer-service {
  width: 42rpx;
  height: 42rpx;
}

.text-content {
  font-size: 32rpx;
  color: white;
}

.btn-contact-tel{
  width: 80%;
  margin-top: 100rpx;
  height: 160rpx;
  background-color: #FFC470;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  border: none;
  border-radius: 16rpx;
}

.btn-contact-tel::after
{
  border: none;
}