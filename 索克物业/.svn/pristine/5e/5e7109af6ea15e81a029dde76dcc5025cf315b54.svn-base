/* 地址 */
.view-top
{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 90rpx;
  width: 92%;
  padding: 0 4% 0 4%;
  background: #fff;
}
.view-address-box
{
  font-size:28rpx;
  color: #323232; 
   display: flex;
  align-items: center;


}
.img-address-icon
{
  width: 25rpx;
  height: 28rpx;
  flex-shrink: 0;
    margin-right: 10rpx;

}
.view-re-loaction
{  font-size:28rpx;
  color: #FE8C21; 
   display: flex;
  align-items: center;

}
.img-re-loaction
{
  width: 37rpx;
  height: 37rpx;
  flex-shrink: 0;
  margin-right: 10rpx;
}
/* 已开通的小区 */
.view-open-village-title
{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 90rpx;
  width: 92%;
  padding: 0 4% 0 4%;
  background: #fff;
    font-size:28rpx;
  color: #323232; 
  margin-top: 10rpx;
}
.img-village
{
    width: 36rpx;
  height: 31rpx;
  flex-shrink: 0;
  margin-right: 10rpx;
}
/* 小区列表 */
.view-village-item
{
    display: flex;
  align-items: center;
  justify-content: space-between;
  height: 110rpx;
  width: 92%;
  padding: 0 4% 0 4%;
  background: #fff;
    font-size:28rpx;
  color: #323232; 
  margin-bottom: 2rpx;
}
.img-arrow {
  width: 10rpx;
  height: 18rpx;
  margin-left: 10rpx;
}
