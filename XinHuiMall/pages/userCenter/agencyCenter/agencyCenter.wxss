/* pages/userCenter/agencycenter/agencycenter.wxss */

page {
  background-color: #fff;
}
.head-line{
 background:rgba(242,242,242,1);
  width: 100%;
  height: 2rpx;

}

.view-agency {
  margin-top: 24rpx;
  margin-bottom: 33rpx;
  width: 258rpx;
  height: 65rpx;
  border-radius: 0 33rpx 33rpx 0;
  background-color: #fc4949;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}

.view-agency-item-left-img {
  width: 188rpx;
  height: 188rpx;
  border-radius: 10rpx;
  /* background-color: red; */
}

/* 列表 */

.view-agency-wrap {
  margin-left: 22rpx;
  margin-right: 22rpx;
}

.view-agency-item {
  display: flex;
  align-content: center;
  justify-content: space-between;
}

.view-agency-item-left {
  margin-right: 26rpx;
}

.view-agency-item-right {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  flex: 1;
}

.view-agency-item-right-commission {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.view-agency-item-right-commission-left {
  color: #999;
}

.view-agency-item-right-commission-right {
  color: #999;
}

.view-agency-item-right-commission-ratio {
  color: #999;
}

.view-agency-item-right-subsidy-ratio {
  color: #999;
}

.text {
  color: #f30;
}
.money{
  color: #FF9900
}
.ratio{
  color: #FC6B4E

}
.view-line{
  width:706rpx;
height:1rpx;
margin-top: 30rpx;
background:rgba(242,242,242,1);
}
